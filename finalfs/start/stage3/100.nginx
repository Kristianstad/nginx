# Set in stage2:
# ---------------------------------------------------------
# set -ef +am
# isFirstRun
# VAR_*
# All functions in /start/functions

if [ "$isFirstRun" == "true" ]
then
	mkdir -p "$VAR_CONFIG_DIR/conf.d" "$VAR_CONFIG_DIR/http.d"
	if [ ! -s "$VAR_CONFIG_DIR/conf.d/default.conf" ]
	then
		createConfdDefault
	fi
	if [ ! -s "$VAR_CONFIG_DIR/http.d/default.conf" ]
	then
		createHttpdDefault
	fi
fi
